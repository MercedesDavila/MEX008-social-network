{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  
  },
  "extends": "eslint:recommended",
  "globals": {
    "firebase": true,
    "filterBy": true,
    "Pokemon": false,
    "example": false,
    "sumWeight": true,
    "computeStats": true,
    "aveWeight": true,
    "assert": false

  },
  "rules": {
    "no-console": 0,
    "semi": 2,
  }
}